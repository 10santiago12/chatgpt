{
  "expo": {
    "name": "Chat-GPT",
    "slug": "Chat-GPT",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whitelogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/whitelogo.png",
        "backgroundColor": "#343541"
      },
      "package": "com.santiago1210.ChatGPT"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/whitelogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/whitelogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#343541"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6cf2fea2-bf6f-42a6-9dbc-0ecbe952fad4"
      }
    }
  }
}
